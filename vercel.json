{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.13.3",
    "DATABASE_URL": "@postgres_url",
    "HUGGINGFACE_TOKEN": "@hf_token"
  }
}